   ],
   "source": [
    "# Finding the numner of non-zero entries in the matrix\n",
    "\n",
    "no_of_ratings_given = np.count_nonzero(final_ratings_matrix)\n",
    "print('Number of ratings given = ', no_of_ratings_given)"
   ]
