   "source": [
    "# CSV - Compressed Sparse Row\n",
    "\n",
    "from scipy.sparse import csr_matrix\n",
    "final_ratings_sparse = csr_matrix(final_ratings_matrix.values)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 44,
   "id": "db376ad4",
   "metadata": {},
   "outputs": [],
   "source": [
    "#Singular value Decomposition\n",
    "U, s, Vt = svds(final_ratings_sparse, k=50) # k is the number of latent features\n",
    "\n",
    "# Construct diagonal array in SVD\n",
    "sigma = np.diag(s)"
   ]
